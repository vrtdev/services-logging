{
  "mappings": {
    "logs": {
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings": {
            "match": "*String",
            "mapping": {
              "type": "text"
            }
          }
        },
        {
          "ints": {
            "match": "*Integer",
            "mapping": {
              "type": "long"
            }
          }
        },
        {
          "audits": {
            "match": "*AuditLogDto",
            "mapping": {
              "properties": {
                "startDate": {
                  "type": "date",
                  "format": "date_optional_time||epoch_millis"
                },
                "duration": {
                  "type": "long"
                },
                "method": {
                  "type": "keyword"
                },
                "className": {
                  "type": "keyword"
                },
                "auditLevel": {
                  "type": "keyword"
                }
              }
            }
          }
        },
        {
          "amqp_transactions": {
            "match": "*AmqpTransactionLogDto",
            "mapping": {
              "properties": {
                "headers": {
                  "properties": {
                    "amqp-method": {
                      "type": "keyword"
                    },
                    "migCorrelationId": {
                      "type": "keyword"
                    },
                    "amqp-url": {
                      "type": "keyword"
                    },
                    "X-Log-Flow-Id": {
                      "type": "keyword"
                    }
                  }
                },
                "startDate": {
                  "type": "date",
                  "format": "date_optional_time||epoch_millis"
                },
                "duration": {
                  "type": "long"
                },
                "transactionId": {
                  "type": "keyword"
                },
                "status": {
                  "type": "keyword"
                },
                "queueName": {
                  "type": "keyword"
                },
                "type": {
                  "type": "keyword"
                },
                "serverName": {
                  "type": "keyword"
                },
                "flowId": {
                  "type": "keyword"
                },
                "exchange": {
                  "type": "keyword"
                },
                "routingKey": {
                  "type": "keyword"
                }
              }
            }
          }
        },
        {
          "http_transactions": {
            "match": "*HttpTransactionLogDto",
            "mapping": {
              "properties": {
                "startDate": {
                  "type": "date",
                  "format": "date_optional_time||epoch_millis"
                },
                "fullUrl": {
                  "type": "keyword"
                },
                "transactionId": {
                  "type": "keyword"
                },
                "httpMethod": {
                  "type": "keyword"
                },
                "status": {
                  "type": "keyword"
                },
                "resource": {
                  "type": "keyword"
                },
                "type": {
                  "type": "keyword"
                },
                "serverName": {
                  "type": "keyword"
                },
                "duration": {
                  "type": "long"
                },
                "responseStatus": {
                  "type": "long"
                },
                "user": {
                  "type": "keyword"
                },
                "flowId": {
                  "type": "keyword"
                }
              }
            }
          }
        }
      ],
      "properties": {
        "environmentInfo": {
          "properties": {
            "runtime": {
              "type": "keyword"
            },
            "startup": {
              "type": "date",
              "format": "date_optional_time||epoch_millis"
            },
            "app": {
              "type": "keyword"
            },
            "env": {
              "type": "keyword"
            }
          }
        },
        "transactionId": {
          "type": "keyword"
        },
        "logDate": {
          "type": "date",
          "format": "date_optional_time||epoch_millis"
        },
        "loggerName": {
          "type": "keyword"
        },
        "date": {
          "type": "date",
          "format": "date_optional_time||epoch_millis"
        },

        "@timestamp": {
          "format": "date_optional_time||epoch_millis",
          "type": "date"
        },
        "logComment": {
          "type": "text"
        },
        "logLevel": {
          "type": "keyword"
        },
        "breadCrumb": {
          "type": "long"
        },
        "hostName": {
          "type": "keyword"
        },
        "lineNumber": {
          "type": "long"
        },
        "className": {
          "type": "keyword"
        },
        "methodName": {
          "type": "keyword"
        },
        "flowId": {
          "type": "keyword"
        }
      }
    }
  }
}
